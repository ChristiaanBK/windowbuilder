<project name="WindowBuilderBuild" default="build">
    <description>
            This Ant script is used to build the Window Builder code
    </description>

    <!-- ================================= 
          target: build              
         ================================= -->
    <target name="build" depends="init" description="This Ant script is used to build the Window Builder code">
    </target>

    <!-- - - - - - - - - - - - - - - - - - 
          target: init                      
         - - - - - - - - - - - - - - - - - -->
    <target name="init">
      <property environment="env" />
    	<echoproperties prefix="env." />
    	<property name="build.root" location="${env.WORKSPACE}/build" />
      <property name="build.tmp" location="${build.root}/tmp" />
    	<property name="build.target" location="${build.root}/target" />
    	<mkdir dir="${build.root}"/>
    	<mkdir dir="${build.tmp}"/>
    </target>

</project>
